<?xml version="1.0" encoding="UTF-8"?>
<project name="module_stylepad1" default="compile.module.stylepad1">
  <dirname property="module.stylepad1.basedir" file="${ant.file.module_stylepad1}"/>
  
  <property name="module.jdk.home.stylepad1" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.stylepad1" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.stylepad1" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.stylepad1" value="-encoding GBK -source 1.6 ${compiler.args}"/>
  
  <property name="stylepad1.output.dir" value="${module.stylepad1.basedir}/../../../../../out/production/Stylepad1"/>
  <property name="stylepad1.testoutput.dir" value="${module.stylepad1.basedir}/../../../../../out/test/Stylepad1"/>
  
  <path id="stylepad1.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="stylepad1.module.production.classpath">
    <path refid="${module.jdk.classpath.stylepad1}"/>
    <path refid="library.lib.classpath"/>
    <path refid="library.lib3.classpath"/>
    <path refid="library.modules1.classpath"/>
  </path>
  
  <path id="stylepad1.runtime.production.module.classpath">
    <pathelement location="${stylepad1.output.dir}"/>
    <path refid="library.lib.classpath"/>
    <path refid="library.lib3.classpath"/>
    <path refid="library.modules1.classpath"/>
  </path>
  
  <path id="stylepad1.module.classpath">
    <path refid="${module.jdk.classpath.stylepad1}"/>
    <pathelement location="${stylepad1.output.dir}"/>
    <path refid="library.lib.classpath"/>
    <path refid="library.lib3.classpath"/>
    <path refid="library.modules1.classpath"/>
  </path>
  
  <path id="stylepad1.runtime.module.classpath">
    <pathelement location="${stylepad1.testoutput.dir}"/>
    <pathelement location="${stylepad1.output.dir}"/>
    <path refid="library.lib.classpath"/>
    <path refid="library.lib3.classpath"/>
    <path refid="library.modules1.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.stylepad1">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.stylepad1">
    <patternset refid="excluded.from.module.stylepad1"/>
  </patternset>
  
  <path id="stylepad1.module.sourcepath">
    <dirset dir="${module.stylepad1.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.stylepad1" depends="compile.module.stylepad1.production,compile.module.stylepad1.tests" description="Compile module Stylepad1"/>
  
  <target name="compile.module.stylepad1.production" depends="register.custom.compilers" description="Compile module Stylepad1; production classes">
    <mkdir dir="${stylepad1.output.dir}"/>
    <javac2 destdir="${stylepad1.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.stylepad1}/javac">
      <compilerarg line="${compiler.args.stylepad1}"/>
      <bootclasspath refid="stylepad1.module.bootclasspath"/>
      <classpath refid="stylepad1.module.production.classpath"/>
      <src refid="stylepad1.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.stylepad1"/>
    </javac2>
    
    <copy todir="${stylepad1.output.dir}">
      <fileset dir="${module.stylepad1.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.stylepad1.tests" depends="register.custom.compilers,compile.module.stylepad1.production" description="compile module Stylepad1; test classes" unless="skip.tests"/>
  
  <target name="clean.module.stylepad1" description="cleanup module">
    <delete dir="${stylepad1.output.dir}"/>
    <delete dir="${stylepad1.testoutput.dir}"/>
  </target>
</project>