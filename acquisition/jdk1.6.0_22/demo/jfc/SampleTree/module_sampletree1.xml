<?xml version="1.0" encoding="UTF-8"?>
<project name="module_sampletree1" default="compile.module.sampletree1">
  <dirname property="module.sampletree1.basedir" file="${ant.file.module_sampletree1}"/>
  
  <property name="module.jdk.home.sampletree1" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.sampletree1" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.sampletree1" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.sampletree1" value="-encoding GBK -source 1.6 ${compiler.args}"/>
  
  <property name="sampletree1.output.dir" value="${module.sampletree1.basedir}/../../../../out/production/SampleTree1"/>
  <property name="sampletree1.testoutput.dir" value="${module.sampletree1.basedir}/../../../../out/test/SampleTree1"/>
  
  <path id="sampletree1.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="sampletree1.module.production.classpath">
    <path refid="${module.jdk.classpath.sampletree1}"/>
    <path refid="library.lib.classpath"/>
    <path refid="library.lib3.classpath"/>
    <path refid="library.modules1.classpath"/>
  </path>
  
  <path id="sampletree1.runtime.production.module.classpath">
    <pathelement location="${sampletree1.output.dir}"/>
    <path refid="library.lib.classpath"/>
    <path refid="library.lib3.classpath"/>
    <path refid="library.modules1.classpath"/>
  </path>
  
  <path id="sampletree1.module.classpath">
    <path refid="${module.jdk.classpath.sampletree1}"/>
    <pathelement location="${sampletree1.output.dir}"/>
    <path refid="library.lib.classpath"/>
    <path refid="library.lib3.classpath"/>
    <path refid="library.modules1.classpath"/>
  </path>
  
  <path id="sampletree1.runtime.module.classpath">
    <pathelement location="${sampletree1.testoutput.dir}"/>
    <pathelement location="${sampletree1.output.dir}"/>
    <path refid="library.lib.classpath"/>
    <path refid="library.lib3.classpath"/>
    <path refid="library.modules1.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.sampletree1">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.sampletree1">
    <patternset refid="excluded.from.module.sampletree1"/>
  </patternset>
  
  <path id="sampletree1.module.sourcepath">
    <dirset dir="${module.sampletree1.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.sampletree1" depends="compile.module.sampletree1.production,compile.module.sampletree1.tests" description="Compile module SampleTree1"/>
  
  <target name="compile.module.sampletree1.production" depends="register.custom.compilers" description="Compile module SampleTree1; production classes">
    <mkdir dir="${sampletree1.output.dir}"/>
    <javac2 destdir="${sampletree1.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.sampletree1}/javac">
      <compilerarg line="${compiler.args.sampletree1}"/>
      <bootclasspath refid="sampletree1.module.bootclasspath"/>
      <classpath refid="sampletree1.module.production.classpath"/>
      <src refid="sampletree1.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.sampletree1"/>
    </javac2>
    
    <copy todir="${sampletree1.output.dir}">
      <fileset dir="${module.sampletree1.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.sampletree1.tests" depends="register.custom.compilers,compile.module.sampletree1.production" description="compile module SampleTree1; test classes" unless="skip.tests"/>
  
  <target name="clean.module.sampletree1" description="cleanup module">
    <delete dir="${sampletree1.output.dir}"/>
    <delete dir="${sampletree1.testoutput.dir}"/>
  </target>
</project>