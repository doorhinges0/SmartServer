<?xml version="1.0" encoding="UTF-8"?>
<project name="module_tableexample" default="compile.module.tableexample">
  <dirname property="module.tableexample.basedir" file="${ant.file.module_tableexample}"/>
  
  <property name="module.jdk.home.tableexample" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.tableexample" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.tableexample" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.tableexample" value="-encoding GBK -source 1.6 ${compiler.args}"/>
  
  <property name="tableexample.output.dir" value="${module.tableexample.basedir}/../../../../out/production/TableExample"/>
  <property name="tableexample.testoutput.dir" value="${module.tableexample.basedir}/../../../../out/test/TableExample"/>
  
  <path id="tableexample.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="tableexample.module.production.classpath">
    <path refid="${module.jdk.classpath.tableexample}"/>
    <path refid="library.lib.classpath"/>
    <path refid="library.lib3.classpath"/>
    <path refid="library.modules1.classpath"/>
  </path>
  
  <path id="tableexample.runtime.production.module.classpath">
    <pathelement location="${tableexample.output.dir}"/>
    <path refid="library.lib.classpath"/>
    <path refid="library.lib3.classpath"/>
    <path refid="library.modules1.classpath"/>
  </path>
  
  <path id="tableexample.module.classpath">
    <path refid="${module.jdk.classpath.tableexample}"/>
    <pathelement location="${tableexample.output.dir}"/>
    <path refid="library.lib.classpath"/>
    <path refid="library.lib3.classpath"/>
    <path refid="library.modules1.classpath"/>
  </path>
  
  <path id="tableexample.runtime.module.classpath">
    <pathelement location="${tableexample.testoutput.dir}"/>
    <pathelement location="${tableexample.output.dir}"/>
    <path refid="library.lib.classpath"/>
    <path refid="library.lib3.classpath"/>
    <path refid="library.modules1.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.tableexample">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.tableexample">
    <patternset refid="excluded.from.module.tableexample"/>
  </patternset>
  
  <path id="tableexample.module.sourcepath">
    <dirset dir="${module.tableexample.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.tableexample" depends="compile.module.tableexample.production,compile.module.tableexample.tests" description="Compile module TableExample"/>
  
  <target name="compile.module.tableexample.production" depends="register.custom.compilers" description="Compile module TableExample; production classes">
    <mkdir dir="${tableexample.output.dir}"/>
    <javac2 destdir="${tableexample.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.tableexample}/javac">
      <compilerarg line="${compiler.args.tableexample}"/>
      <bootclasspath refid="tableexample.module.bootclasspath"/>
      <classpath refid="tableexample.module.production.classpath"/>
      <src refid="tableexample.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.tableexample"/>
    </javac2>
    
    <copy todir="${tableexample.output.dir}">
      <fileset dir="${module.tableexample.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.tableexample.tests" depends="register.custom.compilers,compile.module.tableexample.production" description="compile module TableExample; test classes" unless="skip.tests"/>
  
  <target name="clean.module.tableexample" description="cleanup module">
    <delete dir="${tableexample.output.dir}"/>
    <delete dir="${tableexample.testoutput.dir}"/>
  </target>
</project>