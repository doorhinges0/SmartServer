<?xml version="1.0" encoding="UTF-8"?>
<project name="module_filechooserdemo1" default="compile.module.filechooserdemo1">
  <dirname property="module.filechooserdemo1.basedir" file="${ant.file.module_filechooserdemo1}"/>
  
  <property name="module.jdk.home.filechooserdemo1" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.filechooserdemo1" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.filechooserdemo1" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.filechooserdemo1" value="-encoding GBK -source 1.6 ${compiler.args}"/>
  
  <property name="filechooserdemo1.output.dir" value="${module.filechooserdemo1.basedir}/../../../../out/production/FileChooserDemo1"/>
  <property name="filechooserdemo1.testoutput.dir" value="${module.filechooserdemo1.basedir}/../../../../out/test/FileChooserDemo1"/>
  
  <path id="filechooserdemo1.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="filechooserdemo1.module.production.classpath">
    <path refid="${module.jdk.classpath.filechooserdemo1}"/>
    <path refid="library.lib.classpath"/>
    <path refid="library.lib3.classpath"/>
    <path refid="library.modules1.classpath"/>
  </path>
  
  <path id="filechooserdemo1.runtime.production.module.classpath">
    <pathelement location="${filechooserdemo1.output.dir}"/>
    <path refid="library.lib.classpath"/>
    <path refid="library.lib3.classpath"/>
    <path refid="library.modules1.classpath"/>
  </path>
  
  <path id="filechooserdemo1.module.classpath">
    <path refid="${module.jdk.classpath.filechooserdemo1}"/>
    <pathelement location="${filechooserdemo1.output.dir}"/>
    <path refid="library.lib.classpath"/>
    <path refid="library.lib3.classpath"/>
    <path refid="library.modules1.classpath"/>
  </path>
  
  <path id="filechooserdemo1.runtime.module.classpath">
    <pathelement location="${filechooserdemo1.testoutput.dir}"/>
    <pathelement location="${filechooserdemo1.output.dir}"/>
    <path refid="library.lib.classpath"/>
    <path refid="library.lib3.classpath"/>
    <path refid="library.modules1.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.filechooserdemo1">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.filechooserdemo1">
    <patternset refid="excluded.from.module.filechooserdemo1"/>
  </patternset>
  
  <path id="filechooserdemo1.module.sourcepath">
    <dirset dir="${module.filechooserdemo1.basedir}">
      <include name="src"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.filechooserdemo1" depends="compile.module.filechooserdemo1.production,compile.module.filechooserdemo1.tests" description="Compile module FileChooserDemo1"/>
  
  <target name="compile.module.filechooserdemo1.production" depends="register.custom.compilers" description="Compile module FileChooserDemo1; production classes">
    <mkdir dir="${filechooserdemo1.output.dir}"/>
    <javac2 destdir="${filechooserdemo1.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.filechooserdemo1}/javac">
      <compilerarg line="${compiler.args.filechooserdemo1}"/>
      <bootclasspath refid="filechooserdemo1.module.bootclasspath"/>
      <classpath refid="filechooserdemo1.module.production.classpath"/>
      <src refid="filechooserdemo1.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.filechooserdemo1"/>
    </javac2>
    
    <copy todir="${filechooserdemo1.output.dir}">
      <fileset dir="${module.filechooserdemo1.basedir}/src">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.filechooserdemo1.tests" depends="register.custom.compilers,compile.module.filechooserdemo1.production" description="compile module FileChooserDemo1; test classes" unless="skip.tests"/>
  
  <target name="clean.module.filechooserdemo1" description="cleanup module">
    <delete dir="${filechooserdemo1.output.dir}"/>
    <delete dir="${filechooserdemo1.testoutput.dir}"/>
  </target>
</project>