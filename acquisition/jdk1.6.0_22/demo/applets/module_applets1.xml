<?xml version="1.0" encoding="UTF-8"?>
<project name="module_applets1" default="compile.module.applets1">
  <dirname property="module.applets1.basedir" file="${ant.file.module_applets1}"/>
  
  <property name="module.jdk.home.applets1" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.applets1" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.applets1" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.applets1" value="-encoding GBK -source 1.6 ${compiler.args}"/>
  
  <property name="applets1.output.dir" value="${module.applets1.basedir}/../../../out/production/applets1"/>
  <property name="applets1.testoutput.dir" value="${module.applets1.basedir}/../../../out/test/applets1"/>
  
  <path id="applets1.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="applets1.module.production.classpath">
    <path refid="${module.jdk.classpath.applets1}"/>
    <path refid="library.lib3.classpath"/>
  </path>
  
  <path id="applets1.runtime.production.module.classpath">
    <pathelement location="${applets1.output.dir}"/>
    <path refid="library.lib3.classpath"/>
  </path>
  
  <path id="applets1.module.classpath">
    <path refid="${module.jdk.classpath.applets1}"/>
    <pathelement location="${applets1.output.dir}"/>
    <path refid="library.lib3.classpath"/>
  </path>
  
  <path id="applets1.runtime.module.classpath">
    <pathelement location="${applets1.testoutput.dir}"/>
    <pathelement location="${applets1.output.dir}"/>
    <path refid="library.lib3.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.applets1">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.applets1">
    <patternset refid="excluded.from.module.applets1"/>
  </patternset>
  
  <path id="applets1.module.sourcepath">
    <dirset dir="${module.applets1.basedir}">
      <include name="Animator"/>
      <include name="ArcTest"/>
      <include name="BarChart"/>
      <include name="Blink"/>
      <include name="CardTest"/>
      <include name="Clock"/>
      <include name="DitherTest"/>
      <include name="DrawTest"/>
      <include name="Fractal"/>
      <include name="GraphicsTest"/>
      <include name="GraphLayout"/>
      <include name="ImageMap"/>
      <include name="JumpingBox"/>
      <include name="MoleculeViewer"/>
      <include name="NervousText"/>
      <include name="SimpleGraph"/>
      <include name="SortDemo"/>
      <include name="SpreadSheet"/>
      <include name="TicTacToe"/>
      <include name="WireFrame"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.applets1" depends="compile.module.applets1.production,compile.module.applets1.tests" description="Compile module applets1"/>
  
  <target name="compile.module.applets1.production" depends="register.custom.compilers" description="Compile module applets1; production classes">
    <mkdir dir="${applets1.output.dir}"/>
    <javac2 destdir="${applets1.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.applets1}/javac">
      <compilerarg line="${compiler.args.applets1}"/>
      <bootclasspath refid="applets1.module.bootclasspath"/>
      <classpath refid="applets1.module.production.classpath"/>
      <src refid="applets1.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.applets1"/>
    </javac2>
    
    <copy todir="${applets1.output.dir}">
      <fileset dir="${module.applets1.basedir}/Animator">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.applets1.basedir}/ArcTest">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.applets1.basedir}/BarChart">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.applets1.basedir}/Blink">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.applets1.basedir}/CardTest">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.applets1.basedir}/Clock">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.applets1.basedir}/DitherTest">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.applets1.basedir}/DrawTest">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.applets1.basedir}/Fractal">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.applets1.basedir}/GraphicsTest">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.applets1.basedir}/GraphLayout">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.applets1.basedir}/ImageMap">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.applets1.basedir}/JumpingBox">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.applets1.basedir}/MoleculeViewer">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.applets1.basedir}/NervousText">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.applets1.basedir}/SimpleGraph">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.applets1.basedir}/SortDemo">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.applets1.basedir}/SpreadSheet">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.applets1.basedir}/TicTacToe">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.applets1.basedir}/WireFrame">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.applets1.tests" depends="register.custom.compilers,compile.module.applets1.production" description="compile module applets1; test classes" unless="skip.tests"/>
  
  <target name="clean.module.applets1" description="cleanup module">
    <delete dir="${applets1.output.dir}"/>
    <delete dir="${applets1.testoutput.dir}"/>
  </target>
</project>