	<!--	</syscode-dynamic>-->
	<!-- 注意："$" 是固定参数必须的一个定义，不能删除 -->
	<syscode-dynamic>
		<definition>$</definition>
		<value-object>com.sunrise.jop.business.system.config.dictitem.persistent.DictItemVO</value-object>
		<code>dictCode</code>
		<name>dictName</name>
	</syscode-dynamic>	
	
	
	<syscode-local>
		<definition>RATE_STATUS</definition>
		<items>
			<itemvalue code="0">无效</itemvalue>
			<itemvalue code="1">有效</itemvalue>
		</items>
	</syscode-local>
	<syscode-local>
		<definition>TREE_ROOT_NODE</definition>
		<items>
			<itemvalue code="-1">根节点</itemvalue>
		</items>
	</syscode-local>
	<syscode-local>
		<definition>MODULE_TYPE</definition>
		<items>
			<itemvalue code="0">顶层菜单</itemvalue>
			<itemvalue code="1">一级菜单</itemvalue>
			<itemvalue code="2">二级菜单</itemvalue>
			<itemvalue code="3">三级菜单</itemvalue>
		</items>
	</syscode-local>
	<syscode-local>
		<definition>YES_NO</definition>
		<items>
			<itemvalue code="0">否</itemvalue>
			<itemvalue code="1">是</itemvalue>
		</items>
	</syscode-local>
	<syscode-local>
		<definition>PM_PROJECTTYPE</definition>
		<items>
			<itemvalue code="1">设计项目</itemvalue>
			<itemvalue code="2">民建项目</itemvalue>
		</items>
	</syscode-local>
	<syscode-local>
		<definition>PM_PROJECTPRIOR</definition>
		<items>
			<itemvalue code="1">重要紧急</itemvalue>
			<itemvalue code="2">重要不紧急</itemvalue>
			<itemvalue code="3">紧急不重要</itemvalue>
			<itemvalue code="4">不紧急不重要</itemvalue>
		</items>
	</syscode-local>
	<syscode-local>
		<definition>PM_PROJECTSTATUS</definition>
		<items>
			<itemvalue code="open">进行中</itemvalue>
			<itemvalue code="closed">已关闭</itemvalue>
			<itemvalue code="notstartup">未启动</itemvalue>
		</items>
	</syscode-local>
	<syscode-local>
		<definition>UCP_MODULEOPENTYPE</definition>
		<items>
			<itemvalue code="true">iframe方式打开</itemvalue>
			<itemvalue code="false">nav方式打开</itemvalue>
		</items>
	</syscode-local>
	<syscode-local>
		<definition>WFS_PROCESSTYPE</definition>
		<items>
			<itemvalue code="sendform">发文类</itemvalue>
			<itemvalue code="receiveform">收文类</itemvalue>
			<itemvalue code="projectform">项目类</itemvalue>
		</items>
	</syscode-local>
	<syscode-local>
		<definition>WFS_PROCESSSTATUS</definition>
		<items>
			<itemvalue code="1">启用</itemvalue>
			<itemvalue code="0">停用</itemvalue>
		</items>
	</syscode-local>
	<syscode-local>
		<definition>WFS_URGENCYE</definition>
		<items>
			<itemvalue code="1">一般</itemvalue>
			<itemvalue code="2">急</itemvalue>
			<itemvalue code="3">紧急 </itemvalue>
		</items>
	</syscode-local>
	<syscode-local>
		<definition>WFS_SECRET</definition>
		<items>
			<itemvalue code="1">普通</itemvalue>
			<itemvalue code="2">商秘</itemvalue>
		</items>
	</syscode-local>
	<syscode-local>
		<definition>WFS_STEPDEF</definition>
		<items>
			<itemvalue code="prepare">拟稿</itemvalue>
			<itemvalue code="departmentCirculation">部门领导审批</itemvalue>
		</items>
	</syscode-local>
</syscode-config>
